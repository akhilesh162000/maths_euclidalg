    "import math\n",
    "def eucalg(x,y):\n",
    "    if x==0:\n",
    "        return 0,1\n",
    "    x1,y1=eucalg(y%x,x)\n",
    "    n=y1-(math.floor((y/x))*x1)\n",
    "    m=x1\n",
    "    return n,m\n",
    "    \n",
    "a=int(input(\"enter the first number: \"))\n",
    "b=int(input(\"enter the second number: \"))\n",
    "n,m=eucalg(a,b)\n",
    "print(\"x=\",n,\"and y=\",m)"
   ]
